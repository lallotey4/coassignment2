#include <chrono>
#include <iostream>
#include <cmath> // for fmod
using namespace std;
using namespace std::chrono;
void RunCalculations(){
    double result;
    for(long long i = 0; i < 10000000000LL; i++){
        result = 2.0;
    }
 //5 Ã— 10^9 multiplications
    for(long long i = 0; i < 5000000000LL; i++){
        result = 1.0; // Since 1.0 * 1.0 is constant, directly assign the result
    }
    
