#include <chrono>
#include <cmath>
#include <iostream>
using namespace std;
using namespace std::chrono;
void MemoryBenchmark(){
    int *nums = new int[arrsize];
    int num, i, j;

    for(i = 0; i < 5; i++){
        for(j = 0; j < arrsize; j++){
            num = *(nums + j);
        }
    }

    for(i = 0; i < 5; i++){
        for(j = 0; j < arrsize; j++){
            *(nums + j) = 1;
        }
    }

    delete [] nums;
}

int main(){

    auto start = high_resolution_clock::now();
    MemoryBenchmark();
    auto stop = high_resolution_clock::now();
